{
  "name": "@benzed/doc",
  "version": "0.0.1",
  "main": "dist",
  "scripts": {
    "update": "npx ncu -u -a",
    "lint": "eslint src --fix",
    "generate-docs": "node dist/scripts/generate-docs",
    "generate-docs:dev": "node dist/scripts/generate-docs-dev",
    "babel": "rm -rf dist; mkdir dist; babel src --out-dir dist --copy-files",
    "babel:dev": "npm run babel -- --watch",
    "webpack": "NODE_ENV=production webpack",
    "webpack:dev": "NODE_ENV=development webpack-dev-server",
    "serve": "node ./dist/scripts/serve.js",
    "serve:dev": "NODE_ENV=development nodemon --watch dist/api --watch config ./dist/scripts/serve.js",
    "build": "npm i && npm run lint && npm run babel && npm run webpack && npm run test",
    "start": "npm run build && npm run serve"
  },
  "keywords": [],
  "author": "BenZed",
  "license": "ISC",
  "description": "Documentation for my packages.",
  "dependencies": {
    "@benzed/app": "latest",
    "@benzed/react": "latest",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@benzed/dev": "latest",
    "jsdoc": "^3.5.5",
    "watchr": "^4.0.1"
  }
}
