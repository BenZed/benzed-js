{
  "name": "@benzed/dev",
  "version": "0.7.0",
  "main": "lib",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "mocha --opts .mocha.opts",
    "test:dev": "npx watch 'npm run test' src test/test-create-project ../react/src",
    "babel": "rm -rf lib; mkdir lib; babel src --out-dir lib --copy-files",
    "babel:dev": "npm run babel -- --watch",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "prepublishOnly": "npm run lint && npm run test && npm run babel",
    "lint": "eslint src --fix"
  },
  "bin": {
    "benzed-project": "./scripts/create-project.js"
  },
  "keywords": [],
  "author": "BenZed",
  "license": "ISC",
  "description": "Test helpers for the rest of @benzed packages.",
  "dependencies": {
    "args": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "is-explicit": "^2.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "module-alias": "^2.1.0",
    "nodemon": "^1.18.1",
    "react-test-renderer": "^16.4.1",
    "style-loader": "^0.21.0",
    "styled-components-test-utils": "^0.4.1",
    "targz": "^1.0.1",
