{
  "name": "@benzed/dev",
  "version": "0.9.2",
  "main": "lib",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "mocha --opts .mocha.opts",
    "test:dev": "npx watch 'npm run test' src test/test-create-project ../react/src",
    "babel": "rm -rf lib; mkdir lib; babel src --out-dir lib --copy-files",
    "babel:dev": "npm run babel -- --watch",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "prepublishOnly": "npm run lint && npm run test && npm run babel",
    "lint": "eslint src --fix",
    "generate-docs": "node lib/scripts/generate-docs",
    "generate-docs:dev": "node lib/scripts/generate-docs-dev",
    "webpack": "NODE_ENV=production webpack",
    "webpack:dev": "NODE_ENV=development webpack-dev-server",
    "serve": "node lib/scripts/serve-docs.js",
    "serve:dev": "NODE_ENV=development nodemon --watch lib/documentation/api --watch config lib/scripts/serve-docs.js",
    "build": "npm i && npm run lint && npm run babel && npm run webpack && npm run test",
    "start": "npm run build && npm run serve"
  },
  "bin": {
    "benzed-project": "lib/scripts/create-project.js"
  },
  "keywords": [],
  "author": "BenZed",
  "license": "ISC",
  "description": "Test helpers for the rest of @benzed packages.",
  "dependencies": {
    "args": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webapp-webpack-plugin": "^2.3.0",
    "is-explicit": "^2.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "module-alias": "^2.1.0",
    "nodemon": "^1.18.3",
    "react-test-renderer": "^16.4.2",
    "style-loader": "^0.22.1",
    "styled-components-test-utils": "^0.4.1",
    "targz": "^1.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "ajv-keywords": "^3.2.0",
    "jsdoc": "^3.5.5",
    "watchr": "^4.0.1",
    "core-js": "^2.5.7"
  },
  "devDependencies": {
    "@benzed/app": "^0.5.5",
    "@benzed/async": "^0.11.0",
    "@benzed/immutable": "^1.3.0",
    "@benzed/schema": "^0.6.0",
    "@benzed/string": "^0.7.0",
    "jsdom": "^11.12.0",
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.2"
  }
}
