    "is-explicit": "^3.0.1",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "module-alias": "^2.2.0",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.1",
    "targz": "^1.0.1",
    "url-loader": "^1.1.2",
    "webapp-webpack-plugin": "^2.6.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
