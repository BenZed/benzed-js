    "@benzed/immutable": "^2.0.7",
    "@benzed/schema": "^0.11.7",
    "@benzed/string": "^0.8.4",
    "jsdom": "^14.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-highlight": "^0.12.0",
    "react-markdown": "^4.0.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^4.2.0"
  }
