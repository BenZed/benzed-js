{
  "name": "@benzed/dev",
  "version": "0.12.0",
  "main": "lib",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "mocha --opts .mocha.opts",
    "test:dev": "npx watch 'npm run test' src test/test-create-project ../react/src",
    "babel": "rm -rf lib; mkdir lib; babel src --out-dir lib --copy-files --verbose",
    "babel:dev": "npm run babel -- --watch",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "prepublishOnly": "npm run babel",
    "lint": "eslint src --fix",
    "generate-docs": "node lib/scripts/generate-docs",
    "generate-docs:dev": "node lib/scripts/generate-docs-dev",
    "webpack": "NODE_ENV=production webpack",
    "webpack:dev": "NODE_ENV=development webpack-dev-server",
    "serve": "node lib/scripts/serve-docs.js",
    "serve:dev": "NODE_ENV=development nodemon --watch src/documentation --ignore src/documentation/webpack lib/scripts/serve-docs.js",
    "build": "npm run lint && npm run babel && npm run webpack && npm run test",
    "start": "npm run build && npm run serve"
  },
  "bin": {
    "benzed-project": "lib/scripts/create-project.js"
  },
  "keywords": [],
  "author": "BenZed",
  "license": "ISC",
  "description": "Test helpers for the rest of @benzed packages.",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ajv-keywords": "^3.2.0",
    "args": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "chai": "^4.2.0",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "is-explicit": "^2.1.0",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "module-alias": "^2.1.0",
    "nodemon": "^1.18.6",
    "style-loader": "^0.23.1",
    "styled-components-test-utils": "^0.4.1",
    "targz": "^1.0.1",
    "url-loader": "^1.1.2",
    "webapp-webpack-plugin": "^2.3.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "nedb": "^1.8.0",
    "feathers-nedb": "^3.1.0",
    "doctrine": "^3.0.0"
  },
  "devDependencies": {
    "@benzed/app": "^0.8.1",
    "@benzed/async": "^0.12.1",
    "@benzed/immutable": "^1.5.1",
    "@benzed/schema": "^0.11.4",
    "@benzed/string": "^0.8.1",
    "jsdom": "^13.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-highlight": "^0.12.0",
    "react-markdown": "^4.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.1"
  }
}
